{
	"manifest_version": 2,

	"name": "news",  
	"version": "1.0",  
	"description": "news art n", // Описание расширения

	"permissions": [ 
			"https://*/*", 
			"http://*/*" 
					],

	"browser_action": {
		"default_title": "news", // Название кнопки
		"default_icon": "images/icon.png", // Иконка для кнопки
		"default_popup": "popup.html" // Всплывающее окно
	},

	"options_page": "options.html" // Страница настроек
}